<h1>Sign up</h1>

<%= form_for User.new do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.submit %>


<% end %>