<%= form_for @portfolio do |f| %>
    <%= f.label :portfolio_name %>
    <%= f.text_field :portfolio_name %>

    <%= f.label :initial_cash %>
    <%= f.number_field :initial_cash %>

    <%= f.label :user_id %>
    <%= f.collection_select :user_id, @users, :id, :full_name, prompt: true %>
    
    <%= f.submit %>
<% end %>